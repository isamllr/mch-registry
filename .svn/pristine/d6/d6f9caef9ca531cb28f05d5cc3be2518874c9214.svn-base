    function Open()
    {
        $this->CCSEventResult = CCGetEvent($this->CCSEvents, "BeforeBuildSelect", $this->Parent);
        $this->SQL = "SELECT delivery.DeliveryID AS delivery_DeliveryID, FacilityName, Sex, NewBornID, BirthDate, Weight \n\n" .

<!-- BEGIN Record patientSearch -->
<form id="patientSearch" method="post" name="{HTMLFormName}" action="{Action}">
  <table border="0" cellspacing="0" cellpadding="0">
    <tr>
      <td valign="top">
        <table class="Header" border="0" cellspacing="0" cellpadding="0">
          <tr>
            <td class="HeaderLeft"><img border="0" alt="" src="Styles/{CCS_Style}/Images/Spacer.gif"></td> 
            <td class="th"><strong>{res:SearchPatient}</strong></td> 
            <td class="HeaderRight"><img border="0" alt="" src="Styles/{CCS_Style}/Images/Spacer.gif"></td>
          </tr>
        </table>
 
        <table class="Record" cellspacing="0" cellpadding="0">
          <!-- BEGIN Error -->
          <tr class="Error">
            <td colspan="2">{Error}</td>
          </tr>
          <!-- END Error -->
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_PatientID">{res:PatientID}</label></td> 
            <td><input id="patientSearchs_PatientID" value="{s_PatientID}" name="{s_PatientID_Name}"></td>
          </tr>
 
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_GivenName">{res:PregnancyRecNr}</label></td> 
            <td><input id="patientSearchs_PregnancyRecNr" value="{s_PregnancyRecNr}" size="50" name="{s_PregnancyRecNr_Name}"></td>
          </tr>
 
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_GivenName">{res:GivenName}</label></td> 
            <td><input id="patientSearchs_GivenName" value="{s_GivenName}" size="50" name="{s_GivenName_Name}"></td>
          </tr>
 
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_FamilyName">{res:FamilyName}</label></td> 
            <td><input id="patientSearchs_FamilyName" value="{s_FamilyName}" size="50" name="{s_FamilyName_Name}"></td>
          </tr>
 
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_BirthDate">{res:BirthDate}</label></td> 
            <td><input id="patientSearchs_BirthDate" value="{s_BirthDate}" maxlength="100" size="8" name="{s_BirthDate_Name}">
              <!-- BEGIN DatePicker DatePicker_s_BirthDate --><a href="javascript:showDatePicker('{Name}','{FormName}','{DateControl}');" id="patientSearchDatePicker_s_BirthDate"><img id="patientSearchDatePicker_s_BirthDate_Image" border="0" alt="Show Date Picker" src="Styles/{CCS_Style}/Images/DatePicker.gif"></a><!-- END DatePicker DatePicker_s_BirthDate --></td>
          </tr>
 
          <tr class="Controls">
            <td class="th"><label for="patientSearchs_Town">{res:Town}</label></td> 
            <td><input id="patientSearchs_Town" value="{s_Town}" maxlength="50" size="50" name="{s_Town_Name}"></td>
          </tr>
 
          <tr class="Bottom">
            <td colspan="2" align="right">&nbsp; 
              <!-- BEGIN Button Button_DoSearch --><input id="patientSearchButton_DoSearch" class="Button" value="{res:CCS_Search}" alt="{res:CCS_Search}" type="submit" name="{Button_Name}"><!-- END Button Button_DoSearch --></td>
          </tr>
        </table>
      </td>
    </tr>
  </table>
</form>
<!-- END Record patientSearch -->

function AddressActualShowHide(clear)
{
        var patientStreetNameActual = document.getElementById("patientStreetNameActual");
        var patientStreetNrActual = document.getElementById("patientStreetNrActual");                
        var patientTownActual = document.getElementById("patientTownActual");                
    var patients_province_ProvinceActualID = document.getElementById("patients_province_ProvinceActualID");
    var patientsDistrictActualID = document.getElementById("patientDistrictActualID");
    var patientHomePhoneActual = document.getElementById("patientHomePhoneActual");                    

    if(get_AddressActual_radio_value() == 1)
    {
        patientStreetNameActual.style.visibility="visible";
        patientStreetNrActual.style.visibility="visible";                
        patientTownActual.style.visibility="visible";
        patients_province_ProvinceActualID.style.visibility="visible";
        patientsDistrictActualID.style.visibility="visible";
        patientHomePhoneActual.style.visibility="visible";                
        if(clear == true)
        {
                patientStreetNameActual.value = "";
                patientStreetNrActual.value = "";
                patientTownActual.value = "";
                patients_province_ProvinceActualID.value = "";
                patientsDistrictActualID.value = "";
                patientHomePhoneActual.value= "";
                }
        }
    if(get_AddressActual_radio_value() == 0)
    {
        patientStreetNameActual.value = '-';
        patientStreetNrActual.value = '-';
        patientTownActual.value = '-';
        patientsDistrictActualID.selectedIndex = 1;
        patientHomePhoneActual.value= '-';
        patientStreetNameActual.style.visibility="hidden";
        patientStreetNrActual.style.visibility="hidden";                
        patientTownActual.style.visibility="hidden";                
        patients_province_ProvinceActualID.style.visibility="hidden";
        patientsDistrictActualID.style.visibility="hidden";
        patientHomePhoneActual.style.visibility="hidden";  
        }       
}

function DischargedShowHide(clear)
